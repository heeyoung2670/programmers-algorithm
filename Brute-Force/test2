# 소수 찾기
from itertools import permutations

def isPrime(a):
  if a < 2:
    return False
  for i in range(2,a):
    if a % i == 0:
      return False
  return True

def solution(numbers):
    stack = []
    for i in range(1, len(numbers) + 1):
        permute = list(map(''.join, permutations(list(numbers), i)))
        permute = set(map(int, permute))
        for j in permute:
            if isPrime(j):
                stack.append(j)
    answer = len(set(stack))
    return answer
